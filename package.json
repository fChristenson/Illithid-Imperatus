{
  "name": "ring-of-the-ulitharid",
  "version": "1.0.0",
  "description": "Probably the best Mind Flayer experience, *in the world*.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "npx rimraf dist",
    "copy": "npm run copy:stats && npm run copy:meta && npm run copy:iconmap && npm run copy:assets && npm run copy:gui",
    "copy:stats": "node scripts/cp.cjs ./src/Public/RingOfTheUlitharid/Stats ./dist/RingOfTheUlitharid/Public/RingOfTheUlitharid/Stats",
    "copy:meta": "node scripts/cp.cjs ./src/Mods/RingOfTheUlitharid/meta.lsx ./dist/RingOfTheUlitharid/Mods/RingOfTheUlitharid/meta.lsx",
    "copy:iconmap": "node scripts/cp.cjs ./src/Public/RingOfTheUlitharid/Assets/Textures/Icons ./dist/RingOfTheUlitharid/Public/RingOfTheUlitharid/Assets/Textures/Icons",
    "copy:gui": "node scripts/cp.cjs ./src/Public/RingOfTheUlitharid/GUI ./dist/RingOfTheUlitharid/Public/RingOfTheUlitharid/GUI",
    "copy:assets": "node scripts/cp.cjs ./src/Public/Game/GUI/Assets ./dist/RingOfTheUlitharid/Public/Game/GUI/Assets",
    "render": "node scripts/render.cjs",
    "dev:xml": "npx nodemon -x \"npm run render\" -e \"xml template\" -w ./src/Public/RingOfTheUlitharid/RootTemplates",
    "dev:copy": "npx nodemon -x \"npm run copy\" -e \"txt xml dds lsx\" -w ./src/Public",
    "resize:64": "node ./scripts/resize.cjs 64 ./src/Icons/Originals ./src/Icons/64x64",
    "resize:256": "node ./scripts/resize.cjs 256 ./src/Icons/Originals ./src/Icons/256x256",
    "iconconfigmap": "node ./scripts/iconMapConfig.cjs ./src/Icons/64x64 ./src/Public/RingOfTheUlitharid/GUI/Icons_Skills.lsx"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fChristenson/Ring-of-the-Ulitharid.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fChristenson/Ring-of-the-Ulitharid/issues"
  },
  "homepage": "https://github.com/fChristenson/Ring-of-the-Ulitharid#readme",
  "dependencies": {
    "cp-cli": "^2.0.0",
    "ejs": "^3.1.9",
    "jimp": "^0.22.10",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.5"
  }
}
